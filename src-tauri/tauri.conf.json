{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "deepseek-shell",
  "version": "0.1.0",
  "identifier": "com.lenovo.deepseek-shell",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "https://chat.deepseek.com/",
    "beforeBuildCommand": "",
    "frontendDist": "https://chat.deepseek.com/"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "deepseek-shell",
        "width": 400,
        "height": 700,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/deepseek.ico",
      "icons/deepseek.png"
    ],
    "windows": {
      "nsis": {}
    }
  }
}
